from turtle import *
screensize(10000, 10000)
tracer(10)
n = 20
up()
for i in range(10):
    right(120)
    forward(10 * n)
down()
for i in range(7):
    forward(15 * n)
    right(90)
for i in range(5):
    right(60)
    forward(20 * n)
    right(30)
up()
tracer(0)
for x in range(-100, 100):
    for y in range(-100, 100):
        goto(x * n, y * n)
        dot(5, 'red')
done()
